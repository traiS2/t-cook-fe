{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url",
      "NEXT_PUBLIC_API_URL": "@next_public_api_url"
    },
    "build": {
      "env": {
        "DATABASE_URL": "@database_url"
      }
    }
  }
  