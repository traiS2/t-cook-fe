{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "build": {
        "env": {
            "DATABASE_URL": "@postgres_prisma_url"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
